<%include ../partials/header%>
<div class="container">
    <div class="row">
        <div class="col-md-6">
            <h3><%= thisPost.title %></h3>
            <img src="/uploads/userPictures/<%=thisPost.author.pic%>">
            <p>Posted by <%= thisPost.author.firstname + " " + thisPost.author.lastname %></p>
            <P><%= thisPost.tag%> </P>
            <p><%- thisPost.content %></p>
        </div>

    </div>

</div>

<%include ../partials/footer%>